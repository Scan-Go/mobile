{
  "expo": {
    "name": "scan-and-go-mobile",
    "slug": "scan-and-go-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./lib/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "platforms": ["ios", "android", "web"],
    "scheme": "com.muhammedkpln.scanandgo",
    "splash": {
      "image": "./lib/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./lib/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./lib/assets/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-router"]
  }
}
